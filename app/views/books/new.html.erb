<h1> New Book</h1>
<%= form_for :book, url: books_path do |f|  %>
    <p>
      ISBN:<br>
      <%= f.text_field :isbn%>
    </p>
    <p>
      Title:<br>
      <%= f.text_field :title%>
    </p>
    <p>
      Author:<br>
      <%= f.text_field :author %>
    </p>
    <p>
      Description:<br>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.submit "Submit" %>
    </p>
<% end %>