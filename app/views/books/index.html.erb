<h1>List of all books</h1>

<table>
  <tr>
    <td>ISBN</td>
    <td>Title</td>
    <td>Description</td>
    <td>Author</td>
    <td>Checked Out</td>
  </tr>

  <% @books.each do |book| %>
      <tr>
        <td><%= book.isbn %></td>
        <td><%= book.title %></td>
        <td><%= book.description %></td>
        <td><%= book.author %></td>
        <td><%= book.checked_out?  %></td>
      </tr>
  <% end %>
</table>
